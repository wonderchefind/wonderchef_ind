<div id="rent-cart-dropdown" class="rent-cart-dropdown"> 
 <div id="cart-rent" class="cart_tabcontent">
    <div class="no-rent-items">
        <p class="cart_empty">
            Your rent cart is currently empty.
        </p>
        <div class="text-continue">
            <a class="btn btn--primary" aria-label="button" href="{{ routes.all_products_collection_url }}">
                {{ 'cart.general.continue_shopping' | t }}
            </a>
        </div>
    </div>
    <div class="has-rent-items">
        <div class="rent-products-list">  
          Loading...
        </div>

        <div class="rent-summary">                
            <span class="label">
                {{ 'cart.label.total' | t }}:
            </span>
            <span class="price">
                
            </span>
        </div>

        <div class="actions">
            <a class="btn btn--secondary btn--viewCart btn--viewRentCart" aria-label="view cart" href="https://wonderchef.com/cart/?view=rentity">
                View rent cart
            </a>
            {% if settings.terms_conditions %}
                <div class="terms_conditions_wrapper mb-3">
                    <input type="checkbox" id="terms-conditions-dropdowncart" value=""/>
                    <label class="title" for="terms-conditions-dropdowncart">
                        {{ settings.terms_conditions_text }}
                    </label>
                </div>
            {% endif %}
            <!--<a class="btn btn--primary btn--checkoutRentCart" href="https://bff2-183-82-121-175.ngrok.io/proceed-to-checkout">
                {{ 'cart.general.checkout' | t }}
            </a>-->
          <div class="btn-actions">
            <form id="rentityCheckoutDrpdownForm" action="" method="post" novalidate>
              <input type="hidden" id="rentityCartDropdownCheckoutId" name="checkoutId"/>
              <input type="submit" name="checkout" class="btn btn--primary cart__submit cart__submit-control" value="{{ 'cart.general.checkout_two' | t }}">
            </form>
          </div>
        </div>
    </div>
    <div style="text-align: center;">
      <span style="font-size: 9px;font-style: italic;">
        Powered by 
      </span>
      <img style="width: 52px;" src="data:image/png;base64," style="width: 42px;">
    </div>
  </div>

</div>

