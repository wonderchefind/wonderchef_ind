<style>
  .rentity-fieldset{
      background-color: #f1f5fe;
      border: 0;
      border-radius: 6px;
      margin: 0 0 43px;
    padding: 2px 20px 20px 20px;
  }
  .rentity-fieldset input::-webkit-outer-spin-button,
.rentity-fieldset  input::-webkit-inner-spin-button {
    -webkit-appearance: none;
    margin: 0;
  }

  /* Firefox */
  .rentity-fieldset  input[type=number] {
    -moz-appearance: textfield;
  }

  .rentity-fieldset legend{
      background-color: #ffffff;
      padding: 5px 12px;
      border-radius: 4px;
      font-size: 14px;
      border: 1px solid #d5dff1;
      width: 180px;
      font-weight: bold;
  }
  .rentity-fieldset .tenure{
  font-size: 14px;
    cursor: default;
  }
  
.rentity-fieldset .rent{
    margin-bottom: 12px;
}
  .rentity-fieldset .rent>span{
    color: #e47066;
    font-weight: bold;
    font-size: 18px;
  }

  .rentity-fieldset .rQty-group{
    max-width: 144px;
    border-radius: 3px;
    border: 2px solid #d5dff1;
    display: flex;
    width: 100%;
    align-items: center;
    flex-wrap: nowrap;
  }

.rentity-fieldset .rQty-group .button{
  position: relative;
  width: 36px;
  height: 40px;
  display: inline-block;
  vertical-align: middle;
  border-right: 1px solid #cccfd6;
  background-color: #d5dff1;
}

.rentity-fieldset .rQty-group input {
  text-align: center;
  border: none;
  width: 70px;
}

.rentity-fieldset .rQty-group .button.plus {
  border-right: none;
  border-left: 1px solid #cccfd6;
}

.rentity-fieldset .rQty-group .icon{
  padding: 12px;
  width: 100%;
  height: 100%;
  fill: #4d5259;
}

  .rentity-fieldset .action-buttons{
  display: flex;
  }


.rentity-fieldset .action-buttons button{
  border-radius: 3px;
  font-size: 14px;
  display: inline-block;
  vertical-align: middle;
  width: 100%;
  margin-right: 8px;
  padding: 13px 15px 11px;
  cursor: pointer;
  border: 1px solid #e47066;
  font-weight: bold;
  text-transform: uppercase;
}

.rentity-fieldset .btn--primary {
  color: #fff;
  border-color: #e47066;
  background-color: #e47066;
}

.rentity-fieldset .btn--primary:not([disabled]):hover, .btn--primary:focus {
  color: #fff;
  border-color: #c9564c;
  background-color: #c9564c;
}

.rentity-fieldset .btn--secondary {
  color: #e47066;
  border-color: #e47066;
  background-color: #fff;
}

.rentity-fieldset .summary{
  font-size: 14px;
  margin: 12px 0;
}

.rentity-range-slider input[type=range] {
 -webkit-appearance: none;
 display: block;
 margin: 0;
 width: 100%;
 background: transparent;
   padding: 0;
   border: 0;
   min-height: 0;
}
 .rentity-range-slider input[type=range]::-webkit-slider-runnable-track {
     -webkit-appearance: none;
     width: 100%;
     height: 20px;
     color: transparent;
     background: #e47066;
     border-radius: 999px;
     border: none;
}
 .rentity-range-slider input[type=range]::-moz-range-track {
     -webkit-appearance: none;
     width: 100%;
     height: 20px;
     color: transparent;
     background: #e47066;
     border-radius: 999px;
     border: none;
}
 .rentity-range-slider input[type=range]::-ms-track {
     -webkit-appearance: none;
     width: 100%;
     height: 20px;
     color: transparent;
     background: #e47066;
     border-radius: 999px;
     border: none;
}
 .rentity-range-slider input[type=range]::-ms-fill-lower {
     display: none;
}
 .rentity-range-slider input[type=range]::-ms-fill-upper {
     display: none;
}
 .rentity-range-slider input[type=range]::-webkit-slider-thumb {
     -webkit-appearance: none;
     height: 20px;
     width: 20px;
     border-radius: 30px;
     background: #fff;
     box-shadow: 0px 2px 10px -2px black(1);
     border: 1px solid black;
}
 .rentity-range-slider input[type=range]::-moz-range-thumb {
     -webkit-appearance: none;
     height: 20px;
     width: 20px;
     border-radius: 30px;
     background: #fff;
     box-shadow: 0px 2px 10px -2px black(1);
     border: 1px solid black;
}
 .rentity-range-slider input[type=range]::-ms-thumb {
     -webkit-appearance: none;
     height: 20px;
     width: 20px;
     border-radius: 30px;
     background: #fff;
     box-shadow: 0px 2px 10px -2px black(1);
     border: 1px solid black;
}
 .rentity-range-slider .ticks {
     display: flex;
     justify-content: space-between;
     padding: 5px 10px;
}
 .rentity-range-slider .tick {
     font-size: 14px;
     line-height: 14px;
     position: relative;
     display: flex;
     justify-content: center;
     width: 1px;
     background: gray;
     height: 5px;
     line-height: 40px;
     margin-bottom: 10px;
}
.rentity-rental-detail-card:not(.loaded) .loader{
  display: block;
}
  
.rentity-rental-detail-card:not(.loaded) .details{
  display: none;
}
  
.rentity-rental-detail-card.loaded  .loader{
  display: none;
}
  
.rentity-rental-detail-card.loaded .details{
  display: block;
}

  </style>
  
