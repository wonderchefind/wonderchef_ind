{%- if product -%} 
{%- assign current_variant = product.selected_or_first_available_variant -%}
{% if product.tags contains settings.rentity_tag_value %}
    <div
      id="rentity-variant-{{ current_variant.id }}-capsule"
      class="rentity-capsule"
      data-product-id="{{ product.id | json }}"
      data-product-variant-id="{{ current_variant.id | json }}"
      style="margin-bottom:10px"
         
         >
      <span
        title="Available on Rent from Rentity"
        class="label"
      >
        <img
          style="display:inline-block"
          src="data:image/png;base64,"
          width="22px"
        />
        <span class="rental-option">...</span>
      </span>
    </div>
  {% endif%} 
{%- endif -%}