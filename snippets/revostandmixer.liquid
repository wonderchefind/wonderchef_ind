                height: 22px;
                display: block;
                border: none;
                background: none;
                padding: 0;
                cursor: pointer;
                position: absolute;
                outline: none;
              }

              .yofla360__button .button__icon {
                display: block;
                width: 100%;
                height: 100%;
              }

              .yofla360__button .button__text {
                display: none;
              }

              .yofla360__button.yofla360__button--hidden {
                display: none;
              }

              .yofla360__button.yofla360__button--play {
                left: 5px;
              }

              .yofla360__button.yofla360__button--left {
                left: 32px;
              }

              .yofla360__button.yofla360__button--right {
                left: 59px;
              }

              .yofla360__button.yofla360__button--fullscreen {
                right: 5px;
              }


              .button__icon--play {
                background: no-repeat url(https://cdn.shopify.com/s/files/1/0104/9211/7092/files/button-play.png?10056);
              }

              .yofla360__button:hover .button__icon--play {
