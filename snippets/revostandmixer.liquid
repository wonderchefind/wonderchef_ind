              .yofla360{
                position: relative;
                border: 0px;
                min-width: 300px;
                min-height: 300px;
                max-width: 100%;
                background: white;
                -webkit-tap-highlight-color: rgba(0,0,0,0);
                -webkit-tap-highlight-color: transparent;
              }

              .yofla360.yofla360--noborder{
                border: 0px;
              }

              .yofla360:fullscreen {
                width: 100%;
                height: 100%;
              }

              .yofla360:-webkit-full-screen {
                width: 100vw;
                height: 100vh;
              }

              .yofla360:-moz-full-screen {
                width: 100vw;
                height: 100vh;
              }

              .yofla360:-ms-fullscreen {
                width: 100vw;
                height: 100vh;
              }

              .yofla360:fullscreen {
                width: 100vw;
                height: 100vh;
              }

              .yofla360--controls-below .yofla360__images,
              .yofla360__images{
                position: absolute;
                z-index: 0;
                background: none;
                width: 100%;
                height: calc(100% - 40px); /* height of controls */
                display: block;
                overflow: hidden;
              }

              .yofla360--controls-over .yofla360__images{
                height: 100%;
              }

              .yofla360__controls{
                position: absolute;
                z-index: 1;
                bottom: 3px;
                height: 27px;
                width: 100%;
                background: none;
                display: none;
              }

              .yofla360__button {
