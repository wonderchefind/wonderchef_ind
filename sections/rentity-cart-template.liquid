<div data-section-id="{{ section.id }}" data-section-type="cart-page" class="rent-cart-page">
        <a href="{{ routes.root_url }}" class="text-link continue_shopping_link" aria-label="link">
            {{ 'cart.general.continue_shopping' | t }} {% render 'icon-chevron-right' %}
        </a>

        <div novalidate class="cart cart-template">
          <!-- auto apply coupon code start -->        
   
    <!-- auto apply coupon code end -->  
            <div class="left-col cart_items">
                <div class="box-title">
                    <h3 class="title">
                      {{ 'cart.label.product' | t }} 
                    </h3>
                </div> 
                <div style="text-align: right; 0;font-size: 9px;font-style: italic;font-weight: normal">
                  Powered by <img src="data:image/png;base64," style="width: 42px;">
                </div>                
                <ul class="cart-list">
					<li>Loading...</li>
                </ul>
                            </div>
            
            <div class="right-col">
                <div class="box-title text-center">
                    <h3 class="title">{{ 'cart.label.order_summary' | t }}</h3> 
                </div>
                <div class="total summary-row">
                    <span class="label">
                        <span>
                            Rent Due:
                        </span>
                    </span>
                    <span class="prices rental-dues">
                        
                    </span> 
                 </div>
                <div class="total summary-row">
                    <span class="label">
                        <span>
                            Refundable Deposit:
                        </span>
                    </span>
                    <span class="prices rental-deposit">
                        
                    </span> 
                 </div>
                
                <div class="total summary-total">
                    <span class="label">
                        <span>
                            {{ 'cart.label.total' | t }}:
                        </span>
                    </span>
                    <span class="prices rental-total-price">
                        {{ cart.total_price | money }}
                    </span> 
                 </div>
                <form id="rentityCheckoutPageForm" action="" method="post"> 
                  <div class="btn-actions">
                      <input type="hidden" id="rentityCartPageCheckoutId" name="checkoutId"/>
                      <input type="submit" name="checkout" class="btn btn--primary cart__submit cart__submit-control" value="{{ 'cart.general.checkout_two' | t }}">
                  </div>
                  <div style="font-size:14px">
   
   
  <p style="font-size:16px">After clicking on Proceed To Checkout, you will be taken to <b>rentity.in</b> to complete the transaction.<p>
 
             
            </div>
              </form>
          </div>
        </div>
</div>