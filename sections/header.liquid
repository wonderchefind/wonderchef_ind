          <div class="container">
            {% if section.settings.notice_link == blank %}
              <div class="wrapper">
              {% else %}
                <a href="{{ section.settings.notice_link }}" class="wrapper">
                {% endif %}

                <span class="text">{% include 'multilang' with section.settings.notice_text %}</span>
                <span class="time" id="flash-sale-time">{% include 'multilang' with section.settings.notice_time %}</span>
                {% if section.settings.notice_link == blank %}
                </div>
            {% else %}
              </a>
            {% endif %}

          </div>
        </div>
      {% endif %}
    {% endif %}
  </header>

