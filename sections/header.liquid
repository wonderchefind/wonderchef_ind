<div class="halo_mobileNavigation" id="site-nav-mobile">
  <div class="close_menu">
    <a href="javascript:void(0)" class="close">&#215;</a>
  </div>
  <div class="halo_mobileNavigation_wrapper">
    <div class="site-nav-mobile-wrapper one"></div>
    <ul class="site-nav-mobile two">
      {% if section.settings.enable_header_account and shop.customer_accounts_enabled %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            {% if customer %}
              <a href="{{ routes.account_url }}">
                {% include 'icon-user' %}
                <span>{{ customer.name }}</span>
              </a>
            {% else %}
              <a href="{{ routes.account_login_url }}">
                {% include 'icon-user' %}
                <span>{{ 'layout.customer.log_in' | t }}</span>
              </a>
            {% endif %}
          </p>
        </li>
      {% endif %}
      {% if section.settings.enable_header_wishlist %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            <a href="{{ routes.cart_url | split: '/cart' | first }}/pages/wish-list">
              {% include 'icon-heart' %}
              {{ 'wishlist.general.wishlist' | t }}
            </a>
          </p>
        </li>
      {% endif %}
      {% if section.settings.enable_phone %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            <a href="tel:{{section.settings.phone_number}}">
              {% include 'icon-phone' %}
              {{ section.settings.phone_number }}
            </a>
          </p>
        </li>
      {% elsif section.settings.enable_header_phone %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            <a href="tel:{{section.settings.header_phone}}">
              {% include 'icon-phone' %}
              {{ section.settings.header_phone }}
            </a>
          </p>
        </li>
      {% endif %}
      {% if section.settings.enable_email %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            <a class="navUser-action" href="mailto:{{section.settings.email }}">
              <i class="far fa-envelope"></i>
              {% include 'multilang' with section.settings.email %}
            </a>
          </p>
        </li>
      {% endif %}
      {% if section.settings.enable_url_1 %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            <a class="navUser-action" href="{{ section.settings.url_1 }}">
              {% include 'multilang' with section.settings.url_text_1 %}
            </a>
          </p>
        </li>
      {% endif %}
      {% if section.settings.enable_url_2 %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            <a class="navUser-action" href="{{ section.settings.url_2 }}">
              {% include 'multilang' with section.settings.url_text_2 %}
            </a>
          </p>
        </li>
      {% endif %}
      {% if section.settings.enable_url_3 %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            <a class="navUser-action" href="{{ section.settings.url_3 }}">
              {% include 'multilang' with section.settings.url_text_3 %}
            </a>
          </p>
        </li>
      {% endif %}
      {% if section.settings.enable_url_4 %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            <a class="navUser-action" href="{{ section.settings.url_4 }}">
              {% include 'multilang' with section.settings.url_text_4 %}
            </a>
          </p>
        </li>
      {% endif %}
      {% if section.settings.enable_url_4 %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            <a class="navUser-action" href="{{ section.settings.url_5 }}">
              {% include 'multilang' with section.settings.url_text_5 %}
            </a>
          </p>
        </li>
      {% endif %}
      {% if section.settings.enable_short_description %}
        <li class="menu-lv-1 item">
          <p class="nav-action menu__moblie_end">
            <a class="navUser-action" href="#">
              {% include 'multilang' with section.settings.short_description %}
            </a>
          </p>
        </li>
      {% endif %}
    </ul>
    {% if section.settings.enable_multiple_language or section.settings.enable_multiple_currencies %}
      <div class="lang-currency-groups">
        <div class="lang-groups">
          <label class="label-text">
            <span {% if settings.enable_multilang %}data-translate="general.language.title"{% endif %}>
              {{ 'general.language.title' | t }}
            </span>
          </label>
        </div>
        <div class="currency-groups mt-2">
          <label class="label-text">
            <span {% if settings.enable_multilang %}data-translate="general.currency.title"{% endif %}>
              {{ 'general.currency.title' | t }}
            </span>
          </label>
        </div>
      </div>
    {% endif %}
  </div>
</div>

