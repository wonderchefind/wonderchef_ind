    <h1>
      {{ page.title }}
    </h1>
    <div class="feature_divider"></div>

    <div  class="static-page">
      <style>
.store .title {
    text-transform: capitalize;
}
        .available_at_store {
          margin-top: 60px;
        }
        .available_at_store h3 {
          border-bottom: 2px solid #ededed;
          display: inline-block;
        }
        #map-canvas, #panel { height: 600px; }
        #panel { width: 330px; float: left; margin-right: 2px; }
        #panel .feature-filter label {
          font-size: 13px;
          font-weight: 500;
          margin-bottom: 10px;
          text-transform: uppercase;
          width: 100%;
        }
        p.attribution, p.attribution a { color: #666; }
        .store-list .title {
          font-size: 16px;
          margin: 0;    text-transform: capitalize;
        }
        .store-list li {
          border-bottom: 1px solid #e2e2e2;
          list-style: outside none none;
        }
        @media (min-width: 100px) and (max-width: 600px) {
          #panel{
            width: 100%;
            margin: auto;
            float: none;
          }
          #map-canvas{
            clear: both;
            width: 100%;
            margin: auto;
          }
          .container {
            width: calc(100% - 6px);
          }
        }
        .faqAccordion>dd {
        	padding: 76px 0px 20px 0px !important;
        }
      </style>
     

<!--       <div class="clearfix"></div> -->

      {% comment %}
      Regular page content goes here.
      {% endcomment %}
<!--       <div class="rte static-page storelocator-panel">
        <div id="panel"></div>
        <div id="map-canvas"></div>
